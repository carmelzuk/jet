<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">


  <!-- Core modules-->
  <bean id="system"
        class="jet.com.modules.jsystem.JSystem">
  </bean>
  <bean id="menu"
  		class="jet.com.modules.menu.Menu">
  	
  </bean>
  <bean id="command"
  		class="jet.com.modules.command.Command">
  </bean>
  
  <bean id="theme"
  		class="jet.com.modules.theme.ThemeModule">
  </bean>
  
  <!-- Custom modules-->
  <bean id="testModule"
        class="jet.com.impl.modules.TestModule">
  </bean>
  

</beans>