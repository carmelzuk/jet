<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="jet.com.modules.jsystem">

    <class name="SystemSchema" table="SYSTEM">
    	<id name="id">
                        <generator class="native"/>
                </id>
        <property name="name"/>
        <property name="type"/>
        <property name="status"/>
    </class>

</hibernate-mapping>